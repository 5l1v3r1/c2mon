<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>cern.dmn2.client</groupId>
		<artifactId>dmn2-client-pom</artifactId>
		<version>1.0.22</version>
	</parent>

	<groupId>cern.c2mon</groupId>
	<version>1.0.1-SNAPSHOT</version>
	<name>c2mon-publisher-mobicall-deploy</name>
	<artifactId>c2mon-publisher-mobicall-deploy</artifactId>


	<scm>
		<developerConnection>https://svn.cern.ch/reps/tim2/trunk/c2mon/c2mon-deploy/dmn2-publisher-mobicall-deploy</developerConnection>
	</scm>

  <dependencies>
    <dependency>
	  	<groupId>cern.c2mon.c2mon-publisher</groupId>
  		<artifactId>c2mon-publisher-mobicall</artifactId>
	  	<version>[1.0.0,2.0.0]</version>
    </dependency>


    <!-- C2MON Client API dependencies -->



	</dependencies>

	<build>
		<plugins>

			<plugin>
				<artifactId>maven-assembly-plugin</artifactId>
				<configuration>
					<descriptors>
						<descriptor>src/assembly/assembly.xml</descriptor>
					</descriptors>

				</configuration>
				<executions>
					<execution>
						<id>create-assembly-file</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>build-helper-maven-plugin</artifactId>
				<version>1.1</version>
			</plugin>
		</plugins>
	</build>

	<description>MobiCall publisher for C2mon. 

For information on mobicall check http://www.mobilisierung.com</description>
</project>


