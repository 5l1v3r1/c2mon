<config>

	<!--
		This variable determines the global title that is given to the
		application and which is displayed e.g. in the task bar
	-->
	<property name="viewer-title" type="java.lang.String" value="DMN2 Viewer [DEV]" />

	<!--
		Application name which is used to register the TIM Viewer in RBAC
	-->
	<property name="rbac-app-name" type="java.lang.String" value="diamon-console" />

	<!--
		Defines URL to the XML file that shall be used to create the
		navigation tree. This URL can also point to a Perl script.
	-->
	<property name="views-navigation-tree-creator" type="java.lang.String"
		value="http://abwww/~diamondev/dmn2-viewer/cgi-bin/viewlist-dev.cgi" />

	<!--
		Defines the initialization parameter for the
		cern.accsoft.gui.frame.Console
	-->
	<property name="console-logger-configuration" type="java.lang.String"
		value="DEBUG_ENABLED = false; TIMESTAMP_ENABLED=false; BUFFER_SIZE=3000;STACK_TRACE_DEPTH=4" />

	<!--
		This property can be used to load the TIM Viewer with different
		palettes. If not specified, the TIM Viewer looks into the standard URL
		directory: "http://timweb.cern.ch/Views/palettes/"
	-->
	<property name="dashboard-palettes-dir" type="java.lang.String"
		value="http://timweb.cern.ch/Views/palettes/" />

	<!--
		Defines the URL of the C2MON Tag Viewer. The Viewer will append a Tag
		ID at the end of the URL to retrieve more information.
	-->
	<property name="tag-viewer-url" type="java.lang.String"
		value="http://cs-ccr-lasertest:8080/c2mon-web-configviewer/tagviewer/" />

</config>