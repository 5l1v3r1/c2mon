<?xml version="1.0" encoding="UTF-8"?>
<products>
  <product name="tim2-psaccess-control">

    <application name="default" />

    <application name="tim2-psaccess-control-pro" extends="default">
      <deployment installLocation="~timoper/dist/public/html/javaws/tim2-psaccess-control">
        <notification>
          <!-- you can give more than one mail for deployment notification -->
          <mail address="tim-admin@cern.ch" />
        </notification>
        <signJars keyStore="~timoper/rep/c2mon/client/jarsign.properties" />
      </deployment>
    </application>

    <application name="tim2-psaccess-control-test" extends="default">
      <deployment installLocation="~timtest/dist/public/test/html/javaws/tim2-psaccess-control-stable">
        <signJars keyStore="~timtest/rep/c2mon/client/jarsign.properties" />
      </deployment>
    </application>

    <application name="tim2-psaccess-control-dev" extends="default">
      <deployment installLocation="~timtest/dist/public/test/html/javaws/tim2-psaccess-control">
        <signJars keyStore="~timtest/rep/c2mon/client/jarsign.properties" />
      </deployment>
    </application>

  </product>
</products>