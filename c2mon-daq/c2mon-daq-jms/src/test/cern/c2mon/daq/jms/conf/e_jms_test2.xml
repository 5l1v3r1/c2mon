<!--

   Example for one Broker with Bridge interconnected brokers 
 
 -->


   <!--  first broker -->
   <EquipmentUnit id="5250" name="JMS:BROKER1">
      <handler-class-name>automatically-set-by-test</handler-class-name>
      <commfault-tag-id>107211</commfault-tag-id>

      <commfault-tag-value>false</commfault-tag-value>
      <address>tcp://localhost:61616</address>
      <SubEquipmentUnits>
      </SubEquipmentUnits>


      <DataTags>
         <DataTag id="54676" name="BE.TEST:TEST2" control="false">
            <data-type>Float</data-type>
            <DataTagAddress>
               <HardwareAddress
                  class="cern.c2mon.shared.common.datatag.address.impl.SimpleHardwareAddressImpl">
                  <address>TOPIC_PERF_TEST</address>
               </HardwareAddress>
               <time-to-live>3600000</time-to-live>
               <priority>2</priority>
               <guaranteed-delivery>false</guaranteed-delivery>
            </DataTagAddress>
         </DataTag>

         <DataTag id="54677" name="BE.TEST:TEST3" control="false">
            <data-type>Float</data-type>
            <DataTagAddress>
               <HardwareAddress
                  class="cern.c2mon.shared.common.datatag.address.impl.SimpleHardwareAddressImpl">
                  <address>QUEUE_PERF_TEST</address>
               </HardwareAddress>
               <time-to-live>3600000</time-to-live>
               <priority>2</priority>
               <guaranteed-delivery>false</guaranteed-delivery>
            </DataTagAddress>
         </DataTag>

         <!--  cross broker checks -->

         <DataTag id="54611" name="BE.TEST:TEST7" control="false">
            <data-type>Float</data-type>
            <DataTagAddress>
               <HardwareAddress
                  class="cern.c2mon.shared.common.datatag.address.impl.SimpleHardwareAddressImpl">
                  <address>BRIDGE_TOPIC_PERF_TEST;tcp://localhost:61617</address>
               </HardwareAddress>
               <time-to-live>3600000</time-to-live>
               <priority>2</priority>
               <guaranteed-delivery>false</guaranteed-delivery>
            </DataTagAddress>
         </DataTag>

         <DataTag id="54612" name="BE.TEST:TEST8" control="false">
            <data-type>Float</data-type>
            <DataTagAddress>
               <HardwareAddress
                  class="cern.c2mon.shared.common.datatag.address.impl.SimpleHardwareAddressImpl">
                  <address>BRIDGE_QUEUE_PERF_TEST;tcp://localhost:61617</address>
               </HardwareAddress>
               <time-to-live>3600000</time-to-live>
               <priority>2</priority>
               <guaranteed-delivery>false</guaranteed-delivery>
            </DataTagAddress>
         </DataTag>
      </DataTags>
      <CommandTags>
      </CommandTags>
   </EquipmentUnit>



