<EquipmentUnit id="5250" name="E_SSH_SSH1">
	<handler-class-name>automatically-set-by-test</handler-class-name>
	<commfault-tag-id>107211</commfault-tag-id>

	<commfault-tag-value>false</commfault-tag-value>
	<address>default_ssh_key=$HOME/.ssh/id_dsa</address>
	<SubEquipmentUnits />

	<DataTags>
		<DataTag id="154052" name="CM.PRE.APP_SERVER:LOW_DISK_SPACE"
			control="false">
			<data-type>Boolean</data-type>

			<DataTagAddress>
				<HardwareAddress
					class="ch.cern.tim.shared.datatag.address.impl.SSHHardwareAddressImpl">
					<system-call>dummy</system-call>
					<call-interval>-1</call-interval>
					<call-delay>-1</call-delay>
					<protocol>dummy</protocol>
				</HardwareAddress>

				<time-to-live>9999999</time-to-live>
				<priority>7</priority>
				<guaranteed-delivery>true</guaranteed-delivery>
			</DataTagAddress>
		</DataTag>
	</DataTags>

	<CommandTags>
		<CommandTag id="155962" name="NS.PRE.P_CVRVSPS01_SNT:CMD_RESTART_SRV_OPC">
			<source-timeout>8000</source-timeout>
			<source-retries>0</source-retries>
			<HardwareAddress
				class="ch.cern.tim.shared.datatag.address.impl.SSHHardwareAddressImpl">
				<system-call>curl -k https://cvops/cgi-bin/opcreset.pl -d account=user -d pw='password' -d machine=machineName</system-call>
				<call-interval>-1</call-interval>
				<call-delay>-1</call-delay>
				<protocol>simple-io</protocol>
			</HardwareAddress>
		</CommandTag>
	</CommandTags>
</EquipmentUnit>