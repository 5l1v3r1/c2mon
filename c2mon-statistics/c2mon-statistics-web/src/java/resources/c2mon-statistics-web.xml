<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jee="http://www.springframework.org/schema/jee"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-2.5.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">
	
  <context:annotation-config/>	  
  <context:component-scan base-package="cern.c2mon.tim.statistics.web"/>
  
  <!-- import properties -->
  <import resource="classpath:resources/import-properties.xml"/>
	
	<bean id="daqList" class="cern.c2mon.statistics.web.DaqList"/>
	<bean id="graphCategories" class="cern.c2mon.statistics.web.GraphCategories"/>
	<bean id="chartManager" class="cern.c2mon.statistics.web.ChartManager" init-method="init"/>
  
  <bean id="userRequests" class="cern.c2mon.statistics.web.UserRequests" scope="request"/>
  <bean id="asQualityTable" class="cern.c2mon.statistics.web.tables.AsQualityTable"/>
  <bean id="daqQualityTable" class="cern.c2mon.statistics.web.tables.DaqQualityTable"/>
  
</beans>
