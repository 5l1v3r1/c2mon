<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

  <bean id="tagManager" class="org.easymock.EasyMock"
    factory-method="createMock">
    <constructor-arg value="cern.c2mon.client.core.manager.CoreTagManager" />
  </bean>

  <bean id="commandManager" class="org.easymock.EasyMock"
    factory-method="createMock">
    <constructor-arg value="cern.c2mon.client.core.C2monCommandManager" />
  </bean>
  
</beans>
