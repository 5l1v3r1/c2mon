<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

  <bean id="jmsProxy" class="org.easymock.EasyMock" factory-method="createMock">
    <constructor-arg value="cern.c2mon.client.jms.JmsProxy" />
  </bean>

  <bean id="deviceRequestHandler"
    class="cern.c2mon.client.ext.device.request.DeviceRequestHandler">
  </bean>

  <!-- import jms properties -->
  <import
    resource="classpath:cern/c2mon/client/ext/device/config/jms-properties.xml" />

</beans>
