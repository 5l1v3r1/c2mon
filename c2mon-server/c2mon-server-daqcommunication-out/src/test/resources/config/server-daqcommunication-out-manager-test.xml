<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

  <!-- start module -->
  <context:annotation-config/>
  <import resource="classpath:config/server-daqcommunication-out-connection.xml"/>
  <bean id="processCommunicationManager" class="cern.c2mon.server.daqcommunication.out.impl.ProcessCommunicationManagerImpl"/>
  <bean id="jmsProcessOut" class="cern.c2mon.server.daqcommunication.out.impl.ActiveProcessOut"/>

  <!-- start needed core modules -->
  <import resource="classpath:config/server-cache-nonpersistent-server.xml"/>
  <import resource="classpath:config/server-cachedbaccess-common.xml"/>
  <import resource="classpath:config/server-cacheloading.xml"/>
  <import resource="classpath:test-config/cache/server-test-datasource-hsqldb.xml"/>
  <import resource="classpath:test-config/server-test-properties.xml"/>

</beans>
