<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <import resource="classpath:test-config/cache/server-cache-common-test.xml"/>
  <!-- equipment and process needed for Starting supervision appender -->
  <import resource="classpath:config/server-cache-datatag.xml"/>
  <import resource="classpath:config/server-cachedbaccess-common.xml"/>

  <bean id="supervisionAppender" class="org.easymock.EasyMock" factory-method="createMock">
    <constructor-arg value="cern.c2mon.server.cache.supervision.SupervisionAppender"/>
  </bean>

  <!-- beans needed to test cache -->
  <bean id="dataTagCache" class="cern.c2mon.server.cache.datatag.DataTagCacheImpl" autowire="constructor"/>
  <bean id="dataTagLoaderDAO" class="cern.c2mon.server.cache.loading.impl.DataTagLoaderDAOImpl" autowire="constructor"/>
  <bean id="dataTagCacheObjectFacade" class="cern.c2mon.server.cache.datatag.DataTagCacheObjectFacadeImpl"/>

</beans>
